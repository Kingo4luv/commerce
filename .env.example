# Nuxt 3 Environment Variables Example
# Copy this file to .env and fill in your actual values

# Environment
NUXT_PUBLIC_NODE_ENV=development

# API Configuration
NUXT_PUBLIC_API_URL=https://api.useklump.com/v1
NUXT_PUBLIC_CLIENT_ID=client_id
NUXT_PUBLIC_CLIENT_KEY=client_key
NUXT_PUBLIC_CLIENT_SECRET=client_secret

# Browser Configuration
NUXT_PUBLIC_BROWSER_URL_DOMAIN=http://localhost:3000

# Analytics
NUXT_PUBLIC_GOOGLE_ANALYTICS_ID=google_analytics_id
NUXT_PUBLIC_GOOGLE_ANALYTICS_ID_FOR_STANBIC=G-V16Y1WHNB9

# Payment Configuration
NUXT_PUBLIC_PAGA_MERCHANT_ID=paga_merchant_id
NUXT_PUBLIC_PAYSTACK_PK=paystack_public_key

# Third Party Services
NUXT_PUBLIC_MIX_PANEL_KEY=mixpanel_key
NUXT_PUBLIC_BUGSNAG_API_KEY=bugsnag_api_key
